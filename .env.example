# PhoneClaw Agent - Environment Variables
# Copy this file to .env and fill in your values

# OpenRouter API Key (get one at https://openrouter.ai/keys)
EXPO_PUBLIC_OPENROUTER_API_KEY=

# Optional: Override defaults
# EXPO_PUBLIC_LLM_BASE_URL=https://openrouter.ai/api/v1
# EXPO_PUBLIC_LLM_MODEL=google/gemini-2.0-flash-001

# Telegram Bot Token (from BotFather)
EXPO_PUBLIC_TELEGRAM_BOT_TOKEN=

# Image Model (Optional)
# Defaults to using the main model if not set, or specific model for vision tasks
EXPO_PUBLIC_IMAGE_MODEL=nvidia/nemotron-nano-12b-v2-vl:free
